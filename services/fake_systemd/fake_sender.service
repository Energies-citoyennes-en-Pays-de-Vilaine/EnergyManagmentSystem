[Unit]
Description="fake_sender service"
Wants=fake_sender.timer

[Service]
Type=oneshot
Environment=BASHRCPATH= EMSFOLDER=
ExecStart=/usr/bin/bash -c 'source "$BASHRCPATH"; cd "$EMSFOLDER"; conda activate milp; python -m services.fake_sender'

[Install]
WantedBy=multi-user.target                         